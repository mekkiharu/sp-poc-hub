{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react","prettier"
    ],
    "ignorePatterns": ["*.test.js"],
    "rules": {
        "no-console": "warn",
        "prettier/prettier": "off"
    }
}
